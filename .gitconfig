# This is Git's per-user configuration file.
[user]
 	name = jwbaldwin
 	email = jwbaldwin3@gmail.com
[push]
	default = current
	autoSetupRemote = true
[alias]
	co = checkout
  si = switch
	br = branch
	st = status
	ci = commit
[pull]
	ff = only
[branch]
	autoSetupMerge = false
[core]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
[merge]
	conflictStyle = zdiff3
