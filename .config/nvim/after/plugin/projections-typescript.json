{
  "src/*.ts": {
    "type": "source",
    "alternate": ["src/{}.test.ts", "src/{}.unit.test.ts", "src/{}.spec.ts"]
  },
  "src/*.tsx": {
    "type": "source",
    "alternate": ["src/{}.test.tsx", "src/{}.unit.test.tsx", "src/{}.spec.tsx"]
  },
  "src/*.test.ts": {
    "type": "test",
    "alternate": "src/{}.ts"
  },
  "src/*.unit.test.ts": {
    "type": "test",
    "alternate": "src/{}.ts"
  },
  "src/*.test.tsx": {
    "type": "test",
    "alternate": "src/{}.tsx"
  },
  "src/*.unit.test.tsx": {
    "type": "test",
    "alternate": "src/{}.tsx"
  },
  "src/*.spec.ts": {
    "type": "test",
    "alternate": "src/{}.ts"
  },
  "src/*.spec.tsx": {
    "type": "test",
    "alternate": "src/{}.tsx"
  },
  "app/*.ts": {
    "type": "source",
    "alternate": ["app/{}.test.ts", "app/{}.unit.test.ts", "app/{}.spec.ts"]
  },
  "app/*.tsx": {
    "type": "source",
    "alternate": ["app/{}.test.tsx", "app/{}.unit.test.tsx", "app/{}.spec.tsx"]
  },
  "app/*.test.ts": {
    "type": "test",
    "alternate": "app/{}.ts"
  },
  "app/*.unit.test.ts": {
    "type": "test",
    "alternate": "app/{}.ts"
  },
  "app/*.test.tsx": {
    "type": "test",
    "alternate": "app/{}.tsx"
  },
  "app/*.unit.test.tsx": {
    "type": "test",
    "alternate": "app/{}.tsx"
  },
  "lib/*.ts": {
    "type": "source",
    "alternate": ["lib/{}.test.ts", "lib/{}.unit.test.ts", "lib/{}.spec.ts"]
  },
  "lib/*.test.ts": {
    "type": "test",
    "alternate": "lib/{}.ts"
  },
  "lib/*.unit.test.ts": {
    "type": "test",
    "alternate": "lib/{}.ts"
  },
  "packages/*/src/*.ts": {
    "type": "source",
    "alternate": "packages/{dirname}/src/{basename}.test.ts"
  },
  "packages/*/src/*.test.ts": {
    "type": "test",
    "alternate": "packages/{dirname}/src/{basename}.ts"
  }
}
